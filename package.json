{
  "name": "funnai_frontend",
  "version": "0.1.0",
  "type": "module",
  "description": "Truly self-sovereign AI agents on the Internet Computer",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "dev": "npm run replica && npm run deploy",
    "replica": "dfx stop && dfx start --background",
    "erase-replica": "npm run fresh-replica && npm run deploy",
    "fresh-replica": "dfx stop && rm -rf .dfx && dfx start --clean --background",
    "generate": "dfx generate",
    "deploy": "dfx canister create --all && npm run generate",
    "vite": "vite --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "stop": "dfx stop"
  },
  "dependencies": {
    "@dfinity/agent": "^2.4.1",
    "@dfinity/auth-client": "^2.4.1",
    "@dfinity/authentication": "^0.14.2",
    "@dfinity/candid": "^2.4.1",
    "@dfinity/identity": "^2.4.1",
    "@dfinity/ledger-icp": "^2.6.10",
    "@dfinity/principal": "^2.4.1",
    "@dfinity/utils": "^2.10.1",
    "@mlc-ai/web-llm": "0.2.78",
    "@nfid/identitykit": "^1.0.15",
    "chart.js": "^4.5.0",
    "html5-qrcode": "2.3.8",
    "localforage": "^1.10.0",
    "marked": "^12.0.2",
    "qrcode": "1.5.4",
    "svelte-chartjs": "^3.1.5",
    "svelte-spa-router": "^3.3.0",
    "ua-parser-js": "^1.0.35"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^16.11.6",
    "@types/qrcode": "1.5.5",
    "assert": "2.0.0",
    "autoprefixer": "^10.4.2",
    "buffer": "6.0.3",
    "cssnano": "^5.1.14",
    "events": "3.3.0",
    "lucide-svelte": "0.486.0",
    "postcss": "^8.4.20",
    "postcss-load-config": "^4.0.1",
    "process": "0.11.10",
    "rollup-plugin-visualizer": "^6.0.0",
    "svelte": "^4.0.0",
    "svelte-check": "^3.0.1",
    "svelte-hamburgers": "^4.0.1",
    "svelte-portal": "2.2.1",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "util": "0.12.5",
    "vite": "^4.0.4"
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || ^18"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ],
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.18.20"
  }
}
