type MainerAgentCtrlbCanister = 
 service {
   addChallengeResponseToShareAgent: (challengeResponseSubmissionInput:
    ChallengeResponseSubmissionInput) -> (StatusCodeRecordResult);
   addChallengeToShareServiceQueue: (challengeQueueInput:
    ChallengeQueueInput) -> (ChallengeQueueInputResult);
   addCycles: () -> (AddCyclesResult);
   addMainerShareAgentCanister: (canisterEntryToAdd:
    OfficialMainerAgentCanister) -> (MainerAgentCanisterResult);
   addMainerShareAgentCanisterAdmin: (canisterEntryToAdd:
    OfficialMainerAgentCanister) -> (MainerAgentCanisterResult);
   add_llm_canister: (llmCanisterIdRecord: CanisterIDRecord) ->
    (StatusCodeRecordResult);
   amiController: () -> (StatusCodeRecordResult) query;
   assignAdminRole: (input: AssignAdminRoleInputRecord) ->
    (AdminRoleAssignmentResult);
   canAgentSettingsBeUpdated: () -> (StatusCodeRecordResult);
   checkAccessToLLMs: () -> (StatusCodeRecordResult);
   getAdminRoles: () -> (AdminRoleAssignmentsResult) query;
   getAgentSettingsAdmin: () -> (MainerAgentSettingsListResult) query;
   getAgentTimersAdmin: () -> (MainerAgentTimersListResult) query;
   getChallengeQueueAdmin: () -> (ChallengeQueueInputsResult) query;
   getCurrentAgentSettingsAdmin: () -> (MainerAgentSettingsResult) query;
   getCurrentAgentTimersAdmin: () -> (MainerAgentTimersResult) query;
   getGameStateCanisterId: () -> (text) query;
   getIssueFlagsAdmin: () -> (IssueFlagsRetrievalResult) query;
   getLLMCanisterIds: () -> (CanisterAddressesResult) query;
   getMainerCanisterType: () -> (MainerAgentCanisterTypeResult) query;
   getMainerStatisticsAdmin: () -> (StatisticsRetrievalResult) query;
   getMaintenanceFlag: () -> (FlagResult) query;
   getRecentSubmittedResponsesAdmin: () ->
    (ChallengeResponseSubmissionsResult) query;
   getRoundRobinCanister: () -> (CanisterIDRecordResult) query;
   getShareServiceCanisterId: () -> (text) query;
   getSubmittedResponsesAdmin: () ->
    (ChallengeResponseSubmissionsResult) query;
   getTimerActionRegularityInSecondsAdmin: () -> (MainerTimersResult) query;
   getTimerBufferMaxSizeAdmin: () -> (NatResult) query;
   getTimerBuffersAdmin: () -> (MainerTimerBuffersResult) query;
   get_llm_canisters: () -> (LlmCanistersRecordResult) query;
   health: () -> (StatusCodeRecordResult) query;
   ready: () -> (StatusCodeRecordResult);
   remove_llm_canister: (llmCanisterIdRecord: CanisterIDRecord) ->
    (StatusCodeRecordResult);
   resetChallengeQueueAdmin: () -> (StatusCodeRecordResult);
   resetRoundRobinLLMs: () -> (StatusCodeRecordResult);
   reset_llm_canisters: () -> (StatusCodeRecordResult);
   revokeAdminRole: ("principal": text) -> (TextResult);
   setGameStateCanisterId: (_game_state_canister_id: text) ->
    (StatusCodeRecordResult);
   setMainerCanisterType: (_mainer_agent_canister_type:
    MainerAgentCanisterType) -> (StatusCodeRecordResult);
   setRoundRobinLLMs: (_roundRobinLLMs: nat) -> (StatusCodeRecordResult);
   setShareServiceCanisterId: (_share_service_canister_id: text) ->
    (StatusCodeRecordResult);
   setTimerAction2RegularityInSecondsAdmin: (_action2RegularityInSeconds:
    nat) -> (StatusCodeRecordResult);
   setTimerBufferMaxSizeAdmin: (maxSize: nat) -> (StatusCodeRecordResult);
   startTimerExecutionAdmin: () -> (AuthRecordResult);
   stopTimerExecutionAdmin: () -> (AuthRecordResult);
   timeToNextAgentSettingsUpdate: () -> (NatResult);
   toggleMaintenanceFlagAdmin: () -> (AuthRecordResult);
   triggerChallengeResponseAdmin: () -> (AuthRecordResult);
   updateAgentSettings: (settingsInput: MainerAgentSettingsInput) ->
    (StatusCodeRecordResult);
   whoami: () -> (principal) query;
 };
