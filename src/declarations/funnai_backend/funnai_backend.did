type UserInfoResult = 
 variant {
   Err: ApiError;
   Ok: UserInfo;
 };
type UserInfoInput = record {emailAddress: opt text;};
type UserInfo = 
 record {
   createdAt: nat64;
   emailAddress: opt text;
   isPremiumAccount: bool;
 };
type UserChatSettingsResult = 
 variant {
   Err: ApiError;
   Ok: UserChatSettings;
 };
type UserChatSettings = 
 record {
   responseLength: text;
   saveChats: bool;
   selectedAiModelId: text;
   systemPrompt: text;
   temperature: float64;
 };
type UpdateUserInfoResult = 
 variant {
   Err: ApiError;
   Ok: bool;
 };
type UpdateUserChatSettingsResult = 
 variant {
   Err: ApiError;
   Ok: bool;
 };
type UpdateChatObject = 
 record {
   chatTitle: text;
   id: text;
 };
type SignUpFormInput = 
 record {
   emailAddress: text;
   pageSubmittedFrom: text;
 };
type PaymentInfoInput = record {block_index: nat64;};
type Message = 
 record {
   content: text;
   sender: text;
 };
type MaxMainerTopUpStorageResult = 
 variant {
   Err: ApiError;
   Ok: MaxMainerTopUpStorageResponse;
 };
type MaxMainerTopUpStorageResponse = record {stored: bool;};
type MaxMainerTopUpInput = 
 record {
   amount: nat;
   paymentTransactionBlockId: nat64;
   toppedUpMainerId: text;
 };
type FunnAIBackend = 
 service {
   addMaxMainerTopup: (MaxMainerTopUpInput) -> (MaxMainerTopUpStorageResult);
   create_chat: (vec Message) -> (ChatCreationResult);
   delete_chat: (text) -> (ChatResult);
   delete_email_subscriber: (text) -> (bool);
   get_caller_chat_history: () -> (ChatsPreviewResult) query;
   get_caller_chat_settings: () -> (UserChatSettingsResult) query;
   get_caller_chats: () -> (ChatsResult) query;
   get_caller_user_info: () -> (UserInfoResult) query;
   get_chat: (text) -> (ChatResult) query;
   get_email_subscribers: () -> (vec record {
                                       text;
                                       EmailSubscriber;
                                     }) query;
   greet: (text) -> (text);
   make_caller_account_premium: (PaymentInfoInput) -> (UpdateUserInfoResult);
   submit_signup_form: (SignUpFormInput) -> (text);
   update_caller_chat_settings: (UserChatSettings) ->
    (UpdateUserChatSettingsResult);
   update_caller_user_info: (UserInfoInput) -> (UpdateUserInfoResult);
   update_chat_messages: (text, vec Message) -> (ChatIdResult);
   update_chat_metadata: (UpdateChatObject) -> (ChatIdResult);
 };
type EmailSubscriber = 
 record {
   emailAddress: text;
   pageSubmittedFrom: text;
   subscribedAt: nat64;
 };
type ChatsResult = 
 variant {
   Err: ApiError;
   Ok: vec Chat;
 };
type ChatsPreviewResult = 
 variant {
   Err: ApiError;
   Ok: vec ChatPreview;
 };
type ChatResult = 
 variant {
   Err: ApiError;
   Ok: Chat;
 };
type ChatPreview = 
 record {
   chatTitle: text;
   creationTime: nat64;
   firstMessagePreview: text;
   id: text;
 };
type ChatIdResult = 
 variant {
   Err: ApiError;
   Ok: text;
 };
type ChatCreationResult = 
 variant {
   Err: ApiError;
   Ok: text;
 };
type Chat = 
 record {
   chatTitle: text;
   creationTime: nat64;
   firstMessagePreview: text;
   id: text;
   messages: vec Message;
   owner: principal;
 };
type ApiError = 
 variant {
   InvalidId;
   Other: text;
   Unauthorized;
   ZeroAddress;
 };
service : (principal) -> FunnAIBackend
